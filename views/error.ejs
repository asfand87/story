<% layout('layouts/boilerplate') -%>
<div class="container min-vh-100">
  <div class="row">
    <div class="col-8 offset-2">
      <div class="alert alert-danger" role="alert">
        <% console.log(err) %>
        <h4 class="alert-heading text-center py-2"><%=err.message %></h4>
        <hr />
        <p class="mb-0"><%=err.stack %></p>
      </div>
    </div>
  </div>
</div>
